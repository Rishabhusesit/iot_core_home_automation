# AWS IoT Core Endpoint
# Get this from AWS IoT Console -> Settings -> Device data endpoint
AWS_IOT_ENDPOINT=your-endpoint.iot.region.amazonaws.com

# AWS IoT Port (usually 8883 for MQTT over TLS)
AWS_IOT_PORT=8883

# Thing Name (your IoT device name)
THING_NAME=MyIoTDevice

# Certificate paths (relative to project root)
ROOT_CA_PATH=certificates/AmazonRootCA1.pem
PRIVATE_KEY_PATH=certificates/private.pem.key
CERTIFICATE_PATH=certificates/certificate.pem.crt

# MQTT Topics
TOPIC_PUBLISH=devices/MyIoTDevice/data
TOPIC_SUBSCRIBE=devices/MyIoTDevice/commands

# Client ID
CLIENT_ID=MyIoTDevice

# Quality of Service Level (0, 1, or 2)
QOS_LEVEL=1







